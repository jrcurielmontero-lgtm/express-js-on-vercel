{
  "version": 2,
  "installCommand": "pnpm install --no-frozen-lockfile",
  "builds": [
    { "src": "api/sendToBrevo.js", "use": "@vercel/node" },
    { "src": "api/updateBrevoContact.js", "use": "@vercel/node" },
    { "src": "api/webhook/WebhookSendProposal.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/sendToBrevo", "dest": "api/sendToBrevo.js" },
    { "src": "/api/updateBrevoContact", "dest": "api/updateBrevoContact.js" },
    { "src": "/api/webhook/WebhookSendProposal", "dest": "api/webhooks/WebhookSendProposal.js" }
  ]
}
